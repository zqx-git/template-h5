{
  "name": "vue3-demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode prod",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "eslint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.vue --ignore-path .gitignore",
    "eslint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --fix --ignore-path .gitignore",
    "stylelint": "stylelint \"src/**/*.{css,scss,sass,vue,html}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,sass,vue,html}\" --fix",
    "lint": "npm run eslint & npm run stylelint",
    "lint:fix": "npm run eslint:fix & npm run stylelint:fix"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "eslint-config-prettier": "^8.8.0",
    "pinia": "^2.1.3",
    "pinia-plugin-persist": "^1.0.0",
    "vant": "^4.6.2",
    "vite-plugin-compression": "^0.5.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.3",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-px-to-viewport-8-plugin": "^1.2.2",
    "prettier": "^2.8.8",
    "stylelint": "^15.10.2",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9"
  }
}
